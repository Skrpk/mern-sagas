{
  "presets": ["react", "es2015", "stage-0"],
  "plugins": [
    "react-hot-loader/babel",
    ["css-modules-transform", {
      "generateScopedName": "[name]__[local]__[hash:base64:5]",
      "extensions": [".css"],
      "camelCase": true
  }] 
  ],
  "env": {
    "production": {
      "presets": ["es2015", "react", "react-optimize", "es2015-native-modules", "stage-0"]
    }
  }
}
